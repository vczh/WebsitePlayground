        "tslint": "^5.18.0",
        "tslint-shared": "^1.0.0",
        "typescript": "^3.6.3",
        "webpack": "^4.39.1",
        "webpack-cli": "^3.3.6"
    },
    "dependencies": {
        "gaclib-article": "^0.0.1",
        "gaclib-mvc": "^0.0.1",
        "gaclib-render": "^0.0.1",
        "gaclib-host": "^0.0.1",
